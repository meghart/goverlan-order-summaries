<h1>List all order summaries</h1>
<div class="table-responsive">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Invoice Number</th>
      <th>Account Name</th>
      <th>Salesforce URL</th>
      <th>End-User Email</th>
      <th>Accounts Payable / Reseller Email</th>
      <th>Purchase Type</th>
      <th>Product</th>
      <th>Qty</th>
      <th>Activation</th>
      <th>SUP/UPG Start</th>
      <th>SUP/UPG End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_summaries.each do |order_summary| %>
    <tr>
      <td><%= order_summary.invoice %></td>
      <td><%= order_summary.account_name %></td>
      <td><%= order_summary.salesforce_url %></td>
      <td><%= order_summary.email %></td>
      <td><%= order_summary.reseller %></td>
      <td><%= order_summary.purchase_type %></td>
      <td><%= order_summary.product %></td>
      <td><%= order_summary.quantity %></td>
      <td><%= order_summary.activation %></td>
      <td><%= order_summary.support_start %></td>
      <td><%= order_summary.support_end %></td>
      <td><%= link_to 'Show', order_summary %></td>
      <td><%= link_to 'Edit', edit_order_summary_path(order_summary) %></td>
      <td><%= link_to 'Destroy', order_summary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Order summary', new_order_summary_path, :class => "btn btn-default" %>