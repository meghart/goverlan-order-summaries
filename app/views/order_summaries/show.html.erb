<p id="notice"><%= notice %></p>
<%= link_to order_summaries_path, :class => "btn btn-xs btn-default" do %>
	<i class="fa fa-arrow-left fa-fw"></i>&nbsp;Back
<% end %>
<br />
<h1>Order Summary View</h1>
<div class="row">
  <div class="col-md-4">
  	<h6>Invoice Number:</h6>
  	<h5><%= @order_summary.invoice %></h5>
  </div>
  <div class="col-md-4">
  	<h6>Salesforce URL:</h6>
  	<h5><%= @order_summary.salesforce_url %></h5>
  </div>
  <div class="col-md-4">
    <h6>Account Name:</h6>
    <h5><%= @order_summary.account_name %></h5>
  </div>
</div>
<br />
<div class="row">
  <div class="col-md-4">
  	<h6>End-User Email:</h6>
  	<h5><%= @order_summary.email %></h5>
  </div>
  <div class="col-md-4">
  	<h6>Accounts Payable / Reseller Email:</h6>
  	<h5><%= @order_summary.reseller %></h5>
  </div>
  <div class="col-md-4">
    <h6>Purchase Type:</h6>
    <h5><%= @order_summary.purchase_type %></h5>
  </div>
</div>
<br />
<div class="row">
  <div class="col-md-4">
  	<h6>Product:</h6>
  	<h5><%= @order_summary.product %></h5>
  </div>
  <div class="col-md-4">
  	<h6>Quantity</h6>
  	<h5><%= @order_summary.quantity %></h5>
  </div>
  <div class="col-md-4">
    <h6>Activation</h6>
    <h5><%= @order_summary.activation %></h5>
  </div>
</div>
<br />
<div class="row">
  <div class="col-md-4">
  	<h6>SUP/UPG Start</h6>
  	<h5><%= @order_summary.support_start %></h5>
  </div>
  <div class="col-md-4">
  	<h6>SUP/UPG End</h6>
  	<h5><%= @order_summary.support_end %></h5>
  </div>
  <div class="col-md-4">
  	&nbsp;
  </div>
</div>
<br />
<%= link_to edit_order_summary_path(@order_summary), :class => "btn btn-sm btn-success" do %>
	<i class="fa fa-pencil fa-fw"></i>&nbsp;Edit
<% end %>
