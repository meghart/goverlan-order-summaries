  <tbody>
    <tr>
      <td><%= @order_summary.invoice %></td>
      <td><%= @order_summary.account_name %></td>
      <td><%= @order_summary.salesforce_url %></td>
      <td><%= @order_summary.email %></td>
      <td><%= @order_summary.reseller %></td>
      <td><%= @order_summary.purchase_type %></td>
      <td><%= @order_summary.product %></td>
      <td><%= @order_summary.quantity %></td>
      <td><%= @order_summary.activation %></td>
      <td><%= @order_summary.support_start %></td>
      <td><%= @order_summary.support_end %></td>
      <td><%= link_to 'Edit', edit_order_summary_path(@order_summary) %></td>
    </tr>
  </tbody>
</table>
</div>
<%= link_to 'Back', order_summaries_path %>
